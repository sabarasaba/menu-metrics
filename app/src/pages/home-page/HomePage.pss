.root {
  display: flex;
}

.sparkline {
  margin-top: 2rem;
  background-color: #5969e2;
}

:global .highcharts-credits,
:global .highcharts-legend {
  display: none;
}

:global .highcharts-tooltip > span {
    background: white;
    border: 1px solid silver;
    border-radius: 3px;
    box-shadow: 1px 1px 2px #888;
    padding: 8px;
}
